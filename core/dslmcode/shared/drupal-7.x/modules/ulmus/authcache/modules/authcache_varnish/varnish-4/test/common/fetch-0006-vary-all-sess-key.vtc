varnishtest "Do not cache if backend responds with Vary: * / session / key"

server s1 {
        rxreq
        expect req.url == "/authcache-varnish-get-key"
        expect req.http.X-Forwarded-For == "127.0.0.1"
        txresp -hdr "X-Authcache-Key: 123"

        rxreq
        expect req.url == "/"
        expect req.http.X-Authcache-Key == 123
        expect req.http.X-Forwarded-For == "127.0.0.1"
        txresp -hdr "Vary: *"

        rxreq
        expect req.url == "/"
        expect req.http.X-Authcache-Key == 123
        expect req.http.X-Forwarded-For == "127.0.0.1"
        txresp
} -start

varnish v1 -vcl+backend {
        ${vcl1}
        ${vcl2}
        ${vcl3}
} -start

client c1 {
        txreq -url "/" -hdr "Cookie: SESSabc=efg"
        rxresp
        expect resp.http.Vary == "*"

        txreq -url "/" -hdr "Cookie: SESSabc=efg"
        rxresp
        expect resp.http.Vary == <undef>
}

client c1 -run

# On cache hit for second call to /authcache-varnish-get-key
varnish v1 -expect cache_hit == 1

server s1 -wait
varnish v1 -stop
