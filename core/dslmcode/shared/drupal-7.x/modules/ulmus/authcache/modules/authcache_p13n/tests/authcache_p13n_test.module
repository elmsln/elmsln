/**
 * Menu callback: Modify session data.
 */
function authcache_p13n_test_set_session_data() {
  $data = HookStub::record(__FUNCTION__, func_get_args());
  if ($data) {
    $_SESSION['authcache_p13n_test'] = $data;
  }
  else {
    unset($_SESSION['authcache_p13n_test']);
  }
}

